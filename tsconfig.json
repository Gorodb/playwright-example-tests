{
  "compilerOptions": {
    "target": "ESNext",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "sourceMap": true,
    "types": [
      "@types/jest",
      "@types/node",
      "jest-playwright-preset",
      "@types/allure-js-commons"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
